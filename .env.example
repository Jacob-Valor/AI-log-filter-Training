# =============================================================================
# AI Log Filter - Environment Configuration Template
# =============================================================================
# Copy this file to .env and update values as needed

# Application Settings
APP_ENV=development
LOG_LEVEL=INFO

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:29092
KAFKA_INPUT_TOPIC=raw-logs
KAFKA_CONSUMER_GROUP=ai-log-filter-group

# Model Configuration
MODEL_PATH=/app/models/latest

# Prometheus Metrics
PROMETHEUS_PORT=9090

# Grafana Configuration (CHANGE IN PRODUCTION!)
GF_ADMIN_USER=admin
GF_ADMIN_PASSWORD=changeme

# API Configuration
API_PORT=8000